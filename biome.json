{
	"$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
	"files": {
		"ignore": [
		]
	},
	"organizeImports": {
		"enabled": false
	},
	"formatter": {},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"useLiteralKeys": "off"
			},
			"suspicious": {
				"noExplicitAny": "off"
			}
		}
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true,
		"defaultBranch": "master"
	},
	"javascript": {
		"formatter": {
			"indentStyle": "space",
			"quoteStyle": "single"
		}
	}
}
