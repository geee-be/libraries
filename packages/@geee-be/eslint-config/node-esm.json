{
  "env": {
    "browser": false,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "plugin:node/recommended"
  ],
  "overrides": [
    {
      "files": "reset.d.ts",
      "rules": {
        "spaced-comment": [
          "error",
          "always",
          {
            "line": {
              "markers": [
                "/"
              ]
            }
          }
        ]
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {}
}
