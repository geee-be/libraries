{
  "name": "@geee-be/logger",
  "version": "1.2.1",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "Greg Bacchus",
  "repository": {
    "url": "https://github.com/geee-be/libraries"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist/**/*.js", "dist/**/*.d.ts"],
  "scripts": {
    "build": "tsc",
    "lint": "npm run lint:biome && npm run lint:types && npm run lint:unused-exports",
    "lint:biome": "biome ci src/",
    "lint:types": "tsc --noEmit",
    "lint:unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=src/index.ts",
    "prestart:dev": "rimraf dist/*",
    "start:dev": "tsc --watch"
  },
  "dependencies": {
    "@geee-be/core": "workspace:*",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.5.0",
    "stack-trace": "1.0.0-pre2"
  },
  "devDependencies": {
    "@types/semver": "^7.5.0",
    "@types/stack-trace": "^0.0.30"
  }
}
