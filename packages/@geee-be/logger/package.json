  "type": "module",
  "main": "dist/index.js",
  "types": "src/index.ts",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "lint": "npm run lint:eslint && npm run lint:types && npm run lint:unused-exports",
