{
  "name": "@geee-be/react-twui",
  "version": "1.0.14",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.cts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "concurrently \"npm run build:cjs\" \"npm run build:esm\"",
    "build:cjs": "tsup --format cjs --external react",
    "build:esm": "tsup --format esm --external react",
    "build:watch": "concurrently \"npm:build:watch:*\"",
    "build:watch:cjs": "npm run build:cjs -- --watch",
    "build:watch:esm": "npm run build:esm -- --watch",
    "clean": "rimraf .turbo node_modules dist",
    "dev": "npm run build:watch",
    "lint": "npm run lint:eslint && npm run lint:types && npm run lint:unused-exports",
    "lint:eslint": "eslint src/ --ext .ts,.tsx",
    "lint:eslint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "lint:types": "tsc --noEmit",
    "lint:unused-exports": "ts-unused-exports tsconfig.unused-exports.json --excludePathsFromReport=src/index.ts",
    "storybook": "storybook dev -p 6006 --ci"
  },
  "dependencies": {
    "@geee-be/core": "workspace:*",
    "@geee-be/react-utils": "workspace:*",
    "@iconicicons/react": "latest",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "clsx": "^2.0.0",
    "color": "^4.2.3",
    "color2k": "^2.0.3",
    "cva": "1.0.0-beta.1",
    "deepmerge": "^4.3.1",
    "downshift": "^8.2.3",
    "flat": "^6.0.1",
    "lodash.omit": "^4.5.0",
    "react-hook-form": "^7.50.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@geee-be/eslint-config": "workspace:*",
    "@geee-be/prettier-config": "workspace:*",
    "@happy-dom/global-registrator": "^12.10.3",
    "@storybook/addon-docs": "7.6.6",
    "@storybook/addon-essentials": "7.6.6",
    "@storybook/addon-interactions": "7.6.6",
    "@storybook/addon-links": "7.6.6",
    "@storybook/addon-styling": "1.3.7",
    "@storybook/addons": "7.6.6",
    "@storybook/blocks": "7.6.6",
    "@storybook/channel-postmessage": "7.6.6",
    "@storybook/channel-websocket": "7.6.6",
    "@storybook/client-api": "7.6.6",
    "@storybook/preview-api": "7.6.6",
    "@storybook/preview-web": "7.6.6",
    "@storybook/react": "7.6.6",
    "@storybook/react-vite": "7.6.6",
    "@storybook/testing-library": "0.2.2",
    "@storybook/types": "7.6.6",
    "@swc/core": "^1.3.101",
    "@testing-library/react": "^14.1.2",
    "@types/color": "^3.0.6",
    "@types/jest": "^29.5.11",
    "@types/lodash.omit": "^4.5.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint-plugin-require-extensions": "^0.1.3",
    "eslint-plugin-storybook": "^0.6.15",
    "jest-environment-jsdom": "^29.7.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "7.6.6",
    "storybook-dark-mode": "^3.0.1",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": "*"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    }
  }
}
