{
  "name": "@geee-be/vitest-bdd",
  "version": "2.0.1",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "Greg Bacchus",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc",
    "lint": "npm run lint:biome && npm run lint:types && npm run lint:unused-exports",
    "lint:biome": "biome ci src/",
    "lint:types": "tsc --noEmit",
    "lint:unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=src/index.ts",
    "prestart:dev": "rimraf dist/*",
    "start:dev": "tsc --watch",
    "test": "vitest",
    "test:dev": "vitest --watch --ui"
  },
  "dependencies": {
    "vitest": "^3.0.6"
  },
  "devDependencies": {
    "@vitest/ui": "^3.0.6"
  }
}
